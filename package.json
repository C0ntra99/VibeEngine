{
  "name": "VibeEngine_cli",
  "description": "The cli to setup and configure the VibeEngine.",
  "version": "0.0.0",
  "author": "C0ntra99",
  "bin": {
    "VibeEngine": "./bin/run"
  },
  "bugs": "https://github.com/C0ntra99/VibeEngine/issues",
  "dependencies": {
    "@oclif/command": "^1.5.19",
    "@oclif/config": "^1.13.3",
    "@oclif/plugin-help": "^2.2.3"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1.22.2",
    "@oclif/test": "^1.2.5",
    "chai": "^4.2.0",
    "globby": "^10.0.1",
    "mocha": "^5.2.0",
    "nyc": "^14.1.1"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "/bin",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json",
    "/src"
  ],
  "homepage": "https://github.com/C0ntra99/VibeEngine",
  "keywords": [
    "oclif"
  ],
  "license": "MIT",
  "main": "src/index.js",
  "oclif": {
    "commands": "./src/commands",
    "bin": "VibeEngine",
    "plugins": [
      "@oclif/plugin-help"
    ]
  },
  "repository": "C0ntra99/VibeEngine",
  "scripts": {
    "postpack": "rm -f oclif.manifest.json",
    "prepack": "oclif-dev manifest && oclif-dev readme",
    "test": "nyc mocha --forbid-only \"test/**/*.test.js\"",
    "version": "oclif-dev readme && git add README.md"
  }
}
